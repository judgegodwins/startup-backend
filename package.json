{
  "name": "stread-backend",
  "version": "0.0.1",
  "description": "Stread backend",
  "main": "server.js",
  "author": "Stread",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env APP_ENV=local nodemon ./src/server.ts",
    "build": "rimraf ./build && tsc && cp -R src/lib/mail build/src/lib",
    "test": "NODE_ENV=test APP_ENV=test mocha -r ts-node/register",
    "prettier": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint . --ext .ts --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "generate:migration": "yarn typeorm migration:generate -n",
    "migrate": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "sync:env": "yarn sync-dotenv",
    "prepare": "husky install",
    "seed:db:dev": "cross-env APP_ENV=local nodemon ./src/scripts/seedDatabase.ts",
    "seed:db": "node ./build/src/scripts/seedDatabase.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@sendgrid/mail": "^7.4.4",
    "@supercharge/request-ip": "^1.1.2",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.0.0",
    "bullmq": "^1.40.1",
    "camelcase-keys": "^7.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.4.1",
    "ioredis": "^4.27.6",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.9.20",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.11.1",
    "qs": "^6.9.6",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "snakecase-keys": "^4.0.2",
    "stoppable": "^1.1.0",
    "supertest": "^6.1.3",
    "typeorm": "^0.2.32",
    "useragent": "^2.3.0",
    "winston": "^3.3.3",
    "winston-console-format": "^1.0.8",
    "xlsx": "^0.18.5",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.37",
    "@types/redlock": "^4.0.2",
    "@types/sinon": "^10.0.2",
    "@types/stoppable": "^1.1.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.59.1",
    "chai": "^4.3.4",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "faker": "^5.5.3",
    "husky": "^8.0.0",
    "lint-staged": ">=10",
    "mocha": "^9.1.0",
    "nock": "^13.0.8",
    "nodemon": "^2.0.7",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "supertest": "^6.1.3",
    "sync-dotenv": "^2.6.2",
    "ts-node": "^9.1.1",
    "typescript": "*"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  }
}
